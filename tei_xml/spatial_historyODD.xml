<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml"
	schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
  <teiHeader>
      <fileDesc>
         <titleStmt>
            <title>Spatial History ODD</title>
            <author>Gabi Keane</author>
         </titleStmt>
         <publicationStmt>
            <authority>Prepared by Gabi Keane, with guidance from Elisa Beshero-Bondar for Pitt's World Spatial History internship.</authority>
           
         </publicationStmt>
         <sourceDesc>
            <p>A TEI ODD prepared for newspaper articles on the 1857 Indian Rebellion.</p>
         </sourceDesc>
      </fileDesc>
  </teiHeader>
  <text>
      <body>
         <schemaSpec ident="myTEI">
            <moduleRef key="tei"/>
            <moduleRef key="core" include="date publisher pubPlace p ref foreign"/>
            <moduleRef key="analysis" include="s cl phr w"/>
            <moduleRef key="header" exclude="xenoData revisionDesc"/>
      </schemaSpec>
      </body>
  </text>
</TEI>
